<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Yuan_Catalog>
            <version>0.1.0</version>
        </Yuan_Catalog>
    </modules>
    <global>
        <rewrite>
            <Yuan_Catalog_product>
                <from><![CDATA[#^/catalog/product/#]]></from>
                <to>/catalog/product/</to>
            </Yuan_Catalog_product>
        </rewrite>
    </global>
    <frontend>
        <routers>
            <catalog>
                <use>standard</use>
                <args>
                    <module>Yuan_Catalog</module>
                    <frontName>catalog</frontName>
                </args>
            </catalog>
        </routers>
    </frontend>
</config>
